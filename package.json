{
  "name": "animatron",
  "version": "0.1.0",
  "description": "Various animation helpers for React-based projects",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib && rimraf dist",
    "predeploy": "npm run clean && NODE_ENV=production parcel build examples/index.html --public-url /animatron/",
    "deploy": "gh-pages -d dist",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "start": "DEBUG=true parcel examples/index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "keywords": [
    "animation",
    "react",
    "motion",
    "transition",
    "page"
  ],
  "author": "Matt Styles",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/animatron"
  },
  "bugs": "https://github.com/mattstyles/animatron/issues",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          },
          "loose": true
        }
      ],
      "stage-3",
      "react"
    ],
    "plugins": [
      "styled-components",
      "transform-react-require",
      "transform-class-properties"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/",
      "**/dist/",
      "**/.cache/"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-styled-components": "^1.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-preset-urban": "^1.0.1",
    "gh-pages": "^1.2.0",
    "history": "^4.7.2",
    "open-color": "^1.6.3",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "raid": "^2.13.0",
    "raid-addons": "^2.13.0",
    "raid-navigator": "^2.13.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-transition-group": "^2.4.0",
    "standard": "^12.0.1",
    "styled-components": "^3.4.5"
  },
  "peerDependencies": {
    "react": "^16",
    "react-transition-group": "^2.4.0",
    "styled-components": "^3.4.5"
  },
  "dependencies": {
    "page": "^1.8.6",
    "prop-types": "^15.6.2"
  }
}
