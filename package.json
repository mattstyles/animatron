{
  "name": "animatron",
  "version": "0.2.0",
  "description": "Various animation helpers for React-based projects",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib && rimraf dist",
    "predeploy": "npm run clean && NODE_ENV=production parcel build examples/index.html --public-url /animatron/",
    "deploy": "gh-pages -d dist",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "start": "DEBUG=true parcel examples/index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "keywords": [
    "animation",
    "react",
    "motion",
    "transition",
    "page"
  ],
  "author": "Matt Styles",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/animatron"
  },
  "bugs": "https://github.com/mattstyles/animatron/issues",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          },
          "loose": true
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "styled-components",
      "transform-react-require",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/",
      "**/dist/",
      "**/.cache/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-require": "^1.0.1",
    "gh-pages": "^1.2.0",
    "history": "^4.7.2",
    "open-color": "^1.6.3",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "raid": "^3.0.0",
    "raid-addons": "^3.0.0",
    "raid-navigator": "^3.0.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-transition-group": "^2.5.0",
    "standard": "^12.0.1",
    "styled-components": "^4.1.1"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16",
    "react-transition-group": "^2.5.0",
    "styled-components": "^4.1.1"
  }
}
